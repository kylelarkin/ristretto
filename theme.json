  "styles": {
    "spacing": {
      "padding": {
        "top": "1.5rem",
        "right": "1.5rem",
        "bottom": "1.5rem",
        "left": "1.5rem"
      }
    },
    "color": {
      "text": "var(--wp--preset--color--black)",
      "background": "var(--wp--preset--color--white)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--green-40)"
        }
      },
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans-serif)",
          "fontSize": "var(--wp--preset--font-size--96)",
          "lineHeight": "1.2",
          "fontWeight": "700"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans-serif)",
          "fontSize": "var(--wp--preset--font-size--26)))",
          "lineHeight": "1.2",
          "fontWeight": "700"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans-serif)",
          "fontSize": "22px",
          "lineHeight": "1.3",
          "fontWeight": "700"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "20px",
          "lineHeight": "1.5",
          "fontWeight": "700"
        }
      },
      "h5": {
        "color": {
          "text": "var(--wp--preset--color--gray-80)"
        },
        "typography": {
          "fontSize": "18px",
          "lineHeight": "1.5",
          "fontWeight": "700",
          "textTransform":"uppercase"
        }
      },
      "h6": {
        "color": {
          "text": "var(--wp--preset--color--green-40)"
        },
        "typography": {
          "fontSize": "18px",
          "lineHeight": "var(--wp--custom--typography--line-height--heading)",
          "textTransform":"uppercase"
        }
      }
    },
    "spacing": {
      "blockGap": "24px",
      "padding": {
        "top": "0",
        "right": "var(--wp--preset--spacing--24)",
        "bottom": 0,
        "left": "var(--wp--preset--spacing--24)"
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--sans-serif)",
      "fontSize": "18px",
      "lineHeight": "1.6",
      "heading": {
        "color": "#ffffff"
      }
    }
  }
